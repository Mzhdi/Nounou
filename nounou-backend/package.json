{
    "name": "nounou-nutrition-api",
    "version": "2.0.0",
    "description": "Nounou Nutrition API with MongoDB - AI-powered nutrition tracking and health insights",
    "main": "server.js",
    "scripts": {
        "start": "npm run swagger-gen && node server.js",
        "dev": "npm run swagger-gen && nodemon server.js",
        "dev:debug": "nodemon --inspect server.js",
        "test": "npm run swagger-gen && npm test",
        "test:watch": "jest --watch --detectOpenHandles",
        "test:coverage": "jest --coverage --detectOpenHandles",
        "test:ai": "node test/testAIAnalysis.js",
        "test:ai:verbose": "node test/testAIAnalysis.js --verbose",
        "test:ai:benchmark": "node test/testAIAnalysis.js --benchmark",
        "setup:test-data": "node test/testAIAnalysis.js --create-data",
        "setup:ai": "node scripts/setupAI.js",
        "lint": "eslint . --ext .js",
        "lint:fix": "eslint . --ext .js --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "db:seed": "node scripts/seedDatabase.js",
        "db:migrate": "node scripts/migrateFromPostgres.js",
        "db:backup": "node scripts/backupDatabase.js",
        "db:restore": "node scripts/restoreDatabase.js",
        "db:indexes": "node scripts/createIndexes.js",
        "db:cleanup": "node scripts/cleanupDatabase.js",
        "build": "npm run swagger-gen && echo 'Build complete with documentation'",
        "validate": "npm ls",
        "security:audit": "npm audit",
        "security:fix": "npm audit fix",
        "docs:generate": "node scripts/generateDocs.js",
        "logs:clear": "rm -rf logs/*.log",
        "cluster": "CLUSTER_MODE=true node server.js",
        "pm2:start": "pm2 start ecosystem.config.js",
        "pm2:stop": "pm2 stop ecosystem.config.js",
        "pm2:restart": "pm2 restart ecosystem.config.js",
        "docker:build": "docker build -t nounou-api .",
        "docker:run": "docker run -p 3000:3000 nounou-api",
        "swagger-gen": "node swagger-gen.js",
        "swagger-only": "node swagger-gen.js && echo 'âœ… Swagger docs generated! Visit http://localhost:3000/api-docs'",
        "swagger-watch": "nodemon swagger-gen.js",
        "docs": "npm run swagger-gen && echo 'Documentation generated at /api-docs'"
    },
    "keywords": [
        "nutrition",
        "health",
        "mongodb",
        "express",
        "nodejs",
        "api",
        "ai",
        "food-tracking",
        "nutrition-api",
        "health-app",
        "gemini-ai",
        "image-analysis"
    ],
    "author": {
        "name": "Nounou Team",
        "email": "dev@nounou.app",
        "url": "https://nounou.app"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/nounou-app/nounou-api.git"
    },
    "bugs": {
        "url": "https://github.com/nounou-app/nounou-api/issues"
    },
    "homepage": "https://nounou.app",
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    },
    "dependencies": {
        "@google/generative-ai": "^0.15.0",
        "axios": "^1.6.2",
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "csv-parser": "^3.0.0",
        "csv-writer": "^1.6.0",
        "dotenv": "^16.3.1",
        "exceljs": "^4.4.0",
        "express": "^4.18.2",
        "express-mongo-sanitize": "^2.2.0",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.2.1",
        "helmet": "^7.1.0",
        "hpp": "^0.2.3",
        "joi": "^17.11.0",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "moment-timezone": "^0.5.43",
        "mongoose": "^8.0.3",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.7",
        "pdfkit": "^0.13.0",
        "redis": "^4.6.10",
        "sharp": "^0.32.6",
        "swagger-autogen": "^2.23.7",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "uuid": "^9.0.1",
        "winston": "^3.11.0",
        "xss-clean": "^0.1.4"
    },
    "devDependencies": {
        "@types/jest": "^29.5.8",
        "eslint": "^8.54.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-prettier": "^5.0.1",
        "faker": "^5.5.3",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.0",
        "mongodb-memory-server": "^9.1.3",
        "nodemon": "^3.0.2",
        "prettier": "^3.1.0",
        "supertest": "^6.3.3"
    },
    "jest": {
        "testEnvironment": "node",
        "setupFilesAfterEnv": [
            "<rootDir>/tests/setup.js"
        ],
        "testMatch": [
            "**/__tests__/**/*.js",
            "**/?(*.)+(spec|test).js"
        ],
        "collectCoverageFrom": [
            "**/*.js",
            "!**/node_modules/**",
            "!**/coverage/**",
            "!**/tests/**",
            "!server.js",
            "!ecosystem.config.js"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 70,
                "functions": 70,
                "lines": 70,
                "statements": 70
            }
        }
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "nodemonConfig": {
        "ignore": [
            "node_modules",
            "logs",
            "tests",
            "uploads"
        ],
        "ext": "js,json",
        "env": {
            "NODE_ENV": "development"
        },
        "delay": 1000
    },
    "eslintConfig": {
        "extends": [
            "airbnb-base",
            "prettier"
        ],
        "plugins": [
            "prettier"
        ],
        "rules": {
            "prettier/prettier": "error",
            "no-console": "off",
            "no-unused-vars": [
                "error",
                {
                    "argsIgnorePattern": "^_"
                }
            ],
            "consistent-return": "off",
            "func-names": "off",
            "object-shorthand": "off",
            "no-process-exit": "off",
            "no-param-reassign": "off",
            "no-return-await": "off",
            "no-underscore-dangle": "off",
            "class-methods-use-this": "off",
            "prefer-destructuring": [
                "error",
                {
                    "object": true,
                    "array": false
                }
            ]
        },
        "env": {
            "node": true,
            "es2021": true,
            "jest": true
        }
    },
    "prettier": {
        "semi": true,
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 100,
        "tabWidth": 2,
        "useTabs": false
    }
}